{
  "name": "brackets-electron",
  "productName": "Brackets-Electron",
  "description": "An open source code editor for the web.",
  "author": "Brackets Team <brackets-dev@googlegroups.com>",
  "license": "MIT",
  "homepage": "http://brackets.io",
  "version": "1.8.0",
  "apiVersion": "1.8.0",
  "issues": {
    "url": "http://github.com/zaggino/brackets-electron/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zaggino/brackets-electron.git"
  },
  "main": "./dist/index.js",
  "bin": {
    "brackets": "./dist/index.js"
  },
  "build": {
    "appId": "com.electron.brackets",
    "category": "public.app-category.developer-tools",
    "asar": false,
    "files": [],
    "npmRebuild": true,
    "win": {
      "iconUrl": "https://raw.githubusercontent.com/zaggino/brackets-electron/master/build/icon.ico"
    }
  },
  "directories": {
    "buildResources": "build",
    "app": "dist",
    "output": "dist-build"
  },
  "scripts": {
    "build": "rimraf ./dist && gulp build && tsc --project app && tsc --project src",
    "install": "sh ./pre-rebuild.sh && electron-rebuild -f -e=node_modules/electron && grunt install && npm run build",
    "start": "electron .",
    "pack": "npm run test && build --dir",
    "dist": "npm run test && build",
    "publish-win": "build -w --publish onTagOrDraft",
    "publish-mac": "build -m --publish onTagOrDraft",
    "publish-linux": "build -l --publish onTagOrDraft",
    "dev": "rimraf ./dist && concurrently --kill-others \"gulp watch\" \"tsc --watch --project app\" \"tsc --watch --project src\"",
    "test": "npm run build && npm run tslint",
    "tslint": "tslint -c tslint.json --project tsconfig.json"
  },
  "dependencies": {
    "anymatch": "1.3.0",
    "chokidar": "1.6.0",
    "fs-extra": "0.30.0",
    "isbinaryfile": "3.0.1",
    "lodash": "4.15.0",
    "portscanner": "1.0.0",
    "semver": "5.3.0",
    "strip-bom": "3.0.0",
    "trash": "3.4.1",
    "typescript": "2.0.0",
    "watch": "0.19.2",
    "ws": "0.8.0"
  },
  "optionalDependencies": {
    "fsevents": "1.0.14"
  },
  "devDependencies": {
    "@types/electron": "1.3.19",
    "@types/fs-extra": "0.0.30",
    "@types/lodash": "4.14.33",
    "@types/node": "6.0.37",
    "@types/ws": "0.0.29",
    "concurrently": "2.2.0",
    "electron": "1.3.4",
    "electron-builder": "5.34.1",
    "electron-packager": "7.7.0",
    "electron-rebuild": "1.2.0",
    "eslint": "3.3.1",
    "grunt": "0.4.5",
    "grunt-cleanempty": "1.0.3",
    "grunt-cli": "0.1.9",
    "grunt-contrib-clean": "0.4.1",
    "grunt-contrib-concat": "0.3.0",
    "grunt-contrib-copy": "0.4.1",
    "grunt-contrib-cssmin": "0.6.0",
    "grunt-contrib-htmlmin": "0.1.3",
    "grunt-contrib-jasmine": "0.4.2",
    "grunt-contrib-less": "1.0.1",
    "grunt-contrib-requirejs": "0.4.1",
    "grunt-contrib-uglify": "0.2.0",
    "grunt-contrib-watch": "0.4.3",
    "grunt-eslint": "18.1.0",
    "grunt-jasmine-node": "0.1.0",
    "grunt-lib-phantomjs": "0.3.0",
    "grunt-targethtml": "0.2.6",
    "grunt-template-jasmine-requirejs": "0.1.0",
    "grunt-usemin": "0.1.11",
    "gulp": "3.9.1",
    "gulp-watch": "4.3.9",
    "jasmine-node": "1.11.0",
    "load-grunt-tasks": "3.5.0",
    "phantomjs": "1.9.18",
    "q": "1.4.1",
    "rimraf": "2.5.4",
    "tslint": "3.15.1",
    "xmldoc": "0.1.2"
  }
}
