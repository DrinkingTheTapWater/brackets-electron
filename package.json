{
  "name": "brackets-electron",
  "productName": "Brackets-Electron",
  "description": "An open source code editor for the web.",
  "author": "Brackets Team <brackets-dev@googlegroups.com>",
  "license": "MIT",
  "homepage": "http://brackets.io",
  "version": "1.8.0",
  "apiVersion": "1.8.0",
  "issues": {
    "url": "http://github.com/adobe/brackets/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/brackets.git",
    "branch": "",
    "SHA": ""
  },
  "main": "./dist/index.js",
  "bin": {
    "brackets": "./dist/index.js"
  },
  "build": {
    "appId": "com.electron.brackets",
    "category": "public.app-category.developer-tools",
    "asar": false,
    "files": [],
    "npmRebuild": true,
    "win": {
      "iconUrl": "https://raw.githubusercontent.com/zaggino/brackets-electron/master/build/icon.ico"
    }
  },
  "directories": {
    "buildResources": "build",
    "app": "dist",
    "output": "dist-build"
  },
  "scripts": {
    "build": "rimraf ./dist && gulp build && tsc --project app && tsc --project src",
    "install": "sh ./pre-rebuild.sh && electron-rebuild -e=node_modules/electron && npm run build",
    "start": "electron .",
    "pack": "npm run test && build --dir",
    "dist": "npm run test && build",
    "publish-win": "build -w --publish onTagOrDraft",
    "dev": "rimraf ./dist && concurrently --kill-others \"gulp watch\" \"tsc --watch --project app\" \"tsc --watch --project src\"",
    "test": "npm run build && tslint -c tslint.json --project tsconfig.json"
  },
  "dependencies": {
    "bluebird": "2.10.1",
    "fs-extra": "0.24.0",
    "isbinaryfile": "3.0.0",
    "lodash": "4.15.0",
    "portscanner": "1.0.0",
    "semver": "5.3.0",
    "strip-bom": "2.0.0",
    "trash": "2.0.0",
    "typescript": "2.0.0",
    "watch": "0.19.2",
    "ws": "0.8.0"
  },
  "optionalDependencies": {
    "fsevents": "1.0.14"
  },
  "devDependencies": {
    "@types/electron": "1.3.19",
    "@types/fs-extra": "0.0.30",
    "@types/lodash": "4.14.32",
    "@types/node": "6.0.36",
    "@types/ws": "0.0.29",
    "concurrently": "2.2.0",
    "electron": "1.3.4",
    "electron-builder": "5.30.0",
    "electron-packager": "7.7.0",
    "electron-rebuild": "1.2.0",
    "eslint": "3.3.1",
    "gulp": "3.9.1",
    "gulp-watch": "4.3.9",
    "rimraf": "2.5.4",
    "tslint": "3.14.0"
  }
}
